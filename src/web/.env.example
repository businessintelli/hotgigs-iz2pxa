# Node Environment - Controls application behavior and optimizations
NODE_ENV=development

# Supabase Configuration - Backend integration settings
VITE_SUPABASE_URL=https://<project>.supabase.co
VITE_SUPABASE_ANON_KEY=<your-anon-key>

# API Configuration - Backend API endpoint configuration
VITE_API_URL=http://localhost:3000

# Analytics Configuration - System monitoring integration
VITE_ANALYTICS_ID=<your-analytics-id>

# Error Reporting Configuration - Error tracking system integration
VITE_SENTRY_DSN=<your-sentry-dsn>

# Rate Limiting Configuration - API request limits
VITE_API_RATE_LIMIT_JOBS=1000
VITE_API_RATE_LIMIT_APPLICATIONS=2000
VITE_API_RATE_LIMIT_CANDIDATES=1000
VITE_API_RATE_LIMIT_INTERVIEWS=500

# WebSocket Configuration - Real-time updates
VITE_WEBSOCKET_MAX_RETRIES=5
VITE_WEBSOCKET_RECONNECT_INTERVAL=1000
VITE_WEBSOCKET_HEARTBEAT_INTERVAL=30000

# File Upload Configuration - Document processing limits
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_FILES=5
VITE_UPLOAD_CHUNK_SIZE=1048576

# Cache Configuration - Query caching settings
VITE_CACHE_TTL=900000

# Feature Flags - Optional feature toggles
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_WEBSOCKETS=true

# Region Configuration - Deployment region settings
VITE_REGION=us-east-1

# Version Information - Application versioning
VITE_APP_VERSION=1.0.0